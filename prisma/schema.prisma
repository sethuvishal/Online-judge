generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  Username String @db.VarChar(255)
  id       Int    @id @default(autoincrement())
  email    String @db.VarChar(255)
  password String @db.VarChar(255)
}
